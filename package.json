{
  "name": "First Paint Apps",
  "version": "0.0.1",
  "config": {
    "name": "DSP.KIT",
    "stylesheets": [
      ""
    ]
  },
  "description": "First paint apps",
  "main": "index.js",
  "scripts": {
    "startup": "run-p serve watch:*",
    "start": "run-s clean && run-p 11ty watch:*",
    "serve": "eleventy --serve --incremental",
    "11ty": "eleventy --serve",
    "build": "run-s clean build:* && eleventy",
    "clean": "rm -rf www",
    "trash": "trash www/**/**.*",
    "watch:scss": "sass --style compressed src/sass:src/css/. --load-path=node_modules --watch",
    "build:scss": "sass --style compressed src/sass:src/css/. --load-path=node_modules",
    "build-parcel": "parcel build src/sass/styles.js --no-content-hash --out-dir src/styles/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shawn-sandy/first-paint-apps.git"
  },
  "keywords": [
    "Design",
    "System",
    "11ty",
    "UIKits"
  ],
  "files": [
    "/**"
  ],
  "author": "Shawn Sandy",
  "contributors": [],
